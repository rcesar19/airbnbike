
<%= form_for(@place) do |f| %>

  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :available %>
    <%= f.text_field :available %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
